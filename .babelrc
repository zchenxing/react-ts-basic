{
  "presets": [
    ["@babel/preset-env",
    {
        "targets":{
            "edge":"17",
            "firefox":"60",
            "chrome":"67",
            "safari":"11.1",
            "ie":"11"
        },
        "useBuiltIns":"entry",
        "corejs": "2"
    }],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-object-rest-spread",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ]
}



